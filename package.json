{
  "name": "generator-xsp",
  "version": "0.0.0-semantically-released",
  "description": "a yeoman generator to scaffold an XPages runtime (xsp) compliant On Disk Project (ODP)",
  "homepage": "https://github.com/edm00se/generator-xsp",
  "author": {
    "name": "Eric McCormick",
    "email": "edm00se@gmail.com",
    "url": "https://edm00se.codes/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=4.0.0"
  },
  "keywords": [
    "xpages",
    "yeoman",
    "yo",
    "generator",
    "xsp",
    "domino",
    "ibm",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "2.4.1",
    "change-case": "3.0.2",
    "cheerio": "0.22.0",
    "file-exists": "5.0.1",
    "js-beautify": "1.8.6",
    "lodash": "4.17.11",
    "update-notifier": "2.5.0",
    "yeoman-generator": "3.1.1",
    "yosay": "2.0.2"
  },
  "devDependencies": {
    "all-contributors-cli": "5.4.0",
    "commitizen": "2.10.1",
    "coveralls": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.6.1",
    "eslint-config-xo-space": "0.20.0",
    "eslint-formatter-pretty": "1.3.0",
    "husky": "1.0.1",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "prettier-eslint-cli": "4.7.1",
    "semantic-release": "15.9.16",
    "travis-deploy-once": "5.0.9",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edm00se/generator-xsp.git"
  },
  "scripts": {
    "add": "all-contributors add",
    "generate": "all-contributors generate",
    "test": "jest --coverage",
    "watch": "jest --watchAll --coverage",
    "format": "prettier-eslint \"generators/**/*.js\" \"__tests__/*.js\" --write",
    "lint": "eslint generators/**/*.js __tests__/*.js",
    "precommit": "lint-staged && npm run test",
    "commit": "git-cz"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 90,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
