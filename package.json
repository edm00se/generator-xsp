{
  "name": "generator-xsp",
  "version": "0.0.0-semantically-released",
  "description": "a yeoman generator to scaffold an XPages runtime (xsp) compliant On Disk Project (ODP)",
  "homepage": "https://github.com/edm00se/generator-xsp",
  "author": {
    "name": "Eric McCormick",
    "email": "edm00se@gmail.com",
    "url": "https://ericmccormick.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.0.0"
  },
  "keywords": [
    "xpages",
    "yeoman",
    "yo",
    "generator",
    "xsp",
    "domino",
    "ibm",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "1.1.3",
    "change-case": "3.0.1",
    "cheerio": "0.22.0",
    "file-exists": "3.0.2",
    "js-beautify": "1.6.12",
    "lodash": "4.17.4",
    "update-notifier": "2.1.0",
    "yeoman-generator": "1.1.1",
    "yosay": "2.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "4.0.1",
    "commitizen": "2.9.6",
    "coveralls": "2.12.0",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.18.0",
    "eslint-config-xo-space": "0.15.0",
    "eslint-formatter-pretty": "1.1.0",
    "gulp": "3.9.1",
    "gulp-codecov": "2.0.2",
    "gulp-eslint": "3.0.1",
    "gulp-exclude-gitignore": "1.1.1",
    "gulp-istanbul": "1.1.1",
    "gulp-line-ending-corrector": "1.0.1",
    "gulp-mocha": "3.0.1",
    "gulp-nsp": "2.4.2",
    "gulp-plumber": "1.1.0",
    "husky": "0.13.3-0",
    "semantic-release": "6.3.6",
    "yeoman-assert": "3.0.0",
    "yeoman-test": "1.6.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edm00se/generator-xsp.git"
  },
  "scripts": {
    "add": "all-contributors add",
    "generate": "all-contributors generate",
    "prepublish": "gulp prepublish",
    "test": "gulp",
    "watch": "gulp watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precommit": "npm run test",
    "commit": "git-cz",
    "coveralls": "istanbul check-coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true"
  },
  "license": "MIT"
}
